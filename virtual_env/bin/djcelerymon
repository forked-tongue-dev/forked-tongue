#!/usr/bin/env python2.7

import os; activate_this=os.path.join(os.path.dirname(os.path.realpath(__file__)), 'activate_this.py'); execfile(activate_this, dict(__file__=activate_this)); del os, activate_this

# EASY-INSTALL-ENTRY-SCRIPT: 'django-celery==3.0.17','console_scripts','djcelerymon'
__requires__ = 'django-celery==3.0.17'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('django-celery==3.0.17', 'console_scripts', 'djcelerymon')()
    )